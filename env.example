# 全局配置

# 容器名前缀
GLOBAL_CONTAINER_NAME_PREFIX=of_
# 时区
GLOBAL_TZ=Asia/Shanghai
# 项目源码
GLOBAL_SOURCE_DIR=/Users/hezhongli/data/www/htdocs/officeflow/
# 文件上传目录
GLOBAL_UPLOAD_DIR=./data/nginx/


# nginx

# 镜像版本
NGINX_IMAGE_VERSION=nginx:1.12.1
# 端口
NGINX_HTTP_PORT=80
# 日志目录
NGINX_LOG_DIR=./logs/nginx/
# 配置文件
NGINX_CONF_FILE=./services/nginx/nginx.conf
# fastcgi参数
NGINX_FASTCGI_CONF_FILE=./services/nginx/fastcgi.conf
# 域名
NGINX_PROJECT_DOMAIN=demo.office.rlstech.cn
# 静态资源域名
NGINX_PROJECT_IMGDOMAIN=demo.officeimg.rlstech.cn


# php

# 镜像版本
PHP_IMAGE_VERSION=registry.cn-qingdao.aliyuncs.com/qd_ns/officeflow_php:1.8
# 端口
PHP_FPM_PORT=9000
# 日志目录
PHP_LOG_DIR=./logs/php/
# php-fpm配置文件
PHP_FPM_CONF=./services/php/www.conf
# php.ini配置文件
PHP_INI_CONF=./services/php/php.ini


# redis

# 镜像版本
REDIS_IMAGE_VERSION=redis:4.0.2
# 端口
REDIS_PORT=6379
# 配置文件
REDIS_CONF_FILE=./services/redis/redis.conf
# 日志目录
REDIS_LOG_FILE=./logs/redis/redis.log
# 数据目录
REDIS_DATA_DIR=./data/redis/


# postgres

# 镜像版本
POSTGRES_IMAGE_VERSION=postgres:9.6.0
# 端口
POSTGRES_PORT=5432
# 数据目录
POSTGRES_DATA_DIR=./data/postgres/
# 配置文件
POSTGRES_CONF_FILE=./services/postgres/postgresql.conf
# 用户名
POSTGRES_USERNAME=postgres


# mysql

# 镜像版本
MYSQL_IMAGE_VERSION=mysql:5.6
# 端口
MYSQL_PORT=3306
# 数据目录
MYSQL_DATA_DIR=./data/mysql/
# 配置文件
MYSQL_CONF_FILE=./services/mysql/my.cnf
# 密码
MYSQL_ROOT_PASSWORD=${MYSQL_ROOT_PASSWORD}


# tomcat

# 镜像版本
TOMCAT_IMAGE_VERSION=tomcat:8.5.35
# 端口
TOMCAT_PORT=8080
# 配置
TOMCAT_SERVER_FILE=./services/tomcat/server.xml
# 项目域名
TOMCAT_PROJECT_DOMAIN=demo.activiti.rlstech.cn
